{
  "openapi": "3.0.2",
  "info": {
    "title": "Lagerverwaltung",
    "description": "Service that does the stock management.",
    "termsOfService": "http://BeschreibungDerLagerverwaltung.com/terms/",
    "contact": {
      "name": "Vinzenz Geyer",
      "url": "https://www.hundegger.com",
      "email": "v.geyer@hundegger.com"
    },
    "license": {
      "name": "Apache 2.0",
      "url": "https://www.apache.org/licenses/LICENSE-2.0.html"
    },
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "/"
    }
  ],
  "paths": {
    "/stock/reservation": {
      "post": {
        "tags": [
          "stock"
        ],
        "description": "add a reservation",
        "requestBody": {
          "description": "reservation to add",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Reservation"
              }
            },
            "application/xml": {
              "schema": {
                "$ref": "#/components/schemas/Reservation"
              }
            }
          },
          "required": true
        },
        "responses": {
          "201": {
            "description": "created"
          },
          "405": {
            "description": "Invalid Input -> the reservation is not created"
          },
          "500": {
            "description": "Internal Server Error -> the reservation is not created"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Reservation": {
        "$ref": "#/components/schemas/RawPartType"
      },
      "RawPartType": {
        "type": "object",
        "properties": {
          "dimension": {
            "$ref": "#/components/schemas/Dimension"
          },
          "dimensionZ": {
            "type": "integer",
            "format": "int64"
          },
          "count": {
            "type": "integer",
            "format": "int64"
          }
        }
      },
      "Dimension": {
        "type": "object",
        "properties": {
          "DimensionX": {
            "type": "integer",
            "format": "int64"
          },
          "DimensionY": {
            "type": "integer",
            "format": "int64"
          }
        }
      }
    }
  }
}