{
  "openapi": "3.0.2",
  "info": {
    "title": "Lagerverwaltung",
    "description": "Service that does the stock management.",
    "termsOfService": "http://BeschreibungDerLagerverwaltung.com/terms/",
    "contact": {
      "name": "Vinzenz Geyer",
      "url": "https://www.hundegger.com",
      "email": "v.geyer@hundegger.com"
    },
    "license": {
      "name": "Apache 2.0",
      "url": "https://www.apache.org/licenses/LICENSE-2.0.html"
    },
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "/"
    }
  ],
  "paths": {
    "/stock/reservation": {
      "post": {
        "tags": [
          "stock"
        ],
        "description": "add a reservation",
        "requestBody": {
          "description": "reservation to add",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Reservation"
              }
            },
            "application/xml": {
              "schema": {
                "$ref": "#/components/schemas/Reservation"
              }
            }
          },
          "required": true
        },
        "responses": {
          "201": {
            "description": "created"
          },
          "405": {
            "description": "Invalid Input -> the reservation is not created"
          },
          "500": {
            "description": "Internal Server Error -> the reservation is not created"
          }
        }
      }
    },
    "/stock/reservation/{RawPartType}": {
      "delete": {
        "tags": [
          "stock"
        ],
        "description": "add a reservation",
        "parameters": [
          {
            "name": "RawPartType",
            "in": "path",
            "description": "The Rawpart that is not needed anymore.",
            "required": true,
            "schema": {
              "$ref": "#/components/schemas/Reservation"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "deleted"
          },
          "405": {
            "description": "Invalid Input -> the reservation is not deleted"
          },
          "500": {
            "description": "Internal Server Error -> the reservation is not deleted"
          }
        }
      }
    },
    "/stock/{RawPartType}": {
      "delete": {
        "tags": [
          "stock"
        ],
        "description": "add a reservation",
        "parameters": [
          {
            "name": "RawPartType",
            "in": "path",
            "description": "The Rawpart that is not needed anymore.",
            "required": true,
            "schema": {
              "$ref": "#/components/schemas/RawPartType"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "deleted"
          },
          "405": {
            "description": "Invalid Input -> the reservation is not deleted"
          },
          "500": {
            "description": "Internal Server Error -> the reservation is not deleted"
          }
        }
      }
    },
    "/stock": {
      "get": {
        "tags": [
          "stock"
        ],
        "description": "get the stock",
        "responses": {
          "201": {
            "description": "created",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Stock"
                }
              },
              "application/xml": {
                "schema": {
                  "$ref": "#/components/schemas/Stock"
                }
              }
            }
          },
          "405": {
            "description": "Invalid Input -> the reservation is not created"
          },
          "500": {
            "description": "Internal Server Error -> the reservation is not created"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Id": {
        "type": "string",
        "format": "uuid"
      },
      "Reservation": {
        "$ref": "#/components/schemas/RawPartType"
      },
      "Dimension": {
        "type": "object",
        "properties": {
          "DimensionX": {
            "type": "integer",
            "format": "int64"
          },
          "DimensionY": {
            "type": "integer",
            "format": "int64"
          }
        }
      },
      "RawPartType": {
        "type": "object",
        "properties": {
          "dimension": {
            "$ref": "#/components/schemas/Dimension"
          },
          "dimensionZ": {
            "type": "integer",
            "format": "int64"
          },
          "count": {
            "type": "integer",
            "format": "int64"
          }
        }
      },
      "Stock": {
        "type": "array",
        "items": {
          "$ref": "#/components/schemas/RawPartType"
        }
      }
    }
  }
}
