{
  "openapi": "3.0.2",
  "info": {
    "title": "StockManagementService",
    "description": "Service that does the stock management.",
    "termsOfService": "http://termsOfService.com/terms/",
    "contact": {
      "name": "Vinzenz Geyer",
      "url": "https://www.hundegger.com",
      "email": "v.geyer@hundegger.com"
    },
    "license": {
      "name": "Apache 2.0",
      "url": "https://www.apache.org/licenses/LICENSE-2.0.html"
    },
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "/"
    }
  ],
  "paths": {
    "/stock/availableMaterial": {
      "get": {
        "tags": [
          "stock"
        ],
        "description": "get the stock",
        "responses": {
          "200": {
            "description": "available material",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AvailableMaterial"
                }
              }
            }
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "AvailableMaterial": {
        "type": "array",
        "items": {
          "$ref": "#/components/schemas/Material"
        }
      },
      "Material": {
        "type": "object",
        "properties": {
          "RawPartType": {
            "$ref": "#/components/schemas/RawPartType"
          },
          "Count": {
            "type": "integer",
            "format": "int64"
          }
        }
      },
      "RawPartType": {
        "type": "object",
        "properties": {
          "StrengthClass": {
            "$ref": "#/components/schemas/StrengthClass"
          },
          "Profile": {
            "$ref": "#/components/schemas/Profile"
          },
          "Length": {
            "type": "integer",
            "format": "int64"
          }
        }
      },
      "StrengthClass": {
        "type": "string",
        "enum": [
          "C14",
          "C16",
          "C18",
          "C22",
          "C24",
          "C27",
          "C30",
          "C35",
          "C40",
          "D30",
          "D35",
          "D40",
          "D60",
          "D70",
          "GL20h",
          "GL20c",
          "GL22h",
          "GL22c",
          "GL24h",
          "GL24c",
          "GL26h",
          "GL26c",
          "GL28h",
          "GL28c",
          "GL30h",
          "GL30c",
          "GL32h",
          "GL32c"
        ]
      },
      "Profile": {
        "type": "object",
        "properties": {
          "Height": {
            "type": "integer",
            "format": "int64"
          },
          "Width": {
            "type": "integer",
            "format": "int64"
          }
        }
      }
    }
  }
}